<%- include("partials/header"); -%>

<h2>Projected Savings for the Month</h2>

<!-- Table to display savings data -->
<table class="table">
  <thead>
    <tr>
      <th>Target Savings</th>
      <th>Current Savings</th>
      <th>Percentage Achieved</th>
      <th>Projected Savings</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <!-- Target Savings -->
      <td id="targetSavingsBox">
        <!-- Content for Target Savings goes here -->
      </td>

      <!-- Current Savings -->
      <td id="currentSavingsBox">
        <!-- Content for Current Savings goes here -->
      </td>

      <!-- Percentage Achieved -->
      <td id="savingsProgressBox">
        <!-- Content for Savings Progress goes here -->
      </td>

      <!-- Projected Savings -->
      <td id="savingsPredictionBox">
        <!-- Content for Savings Prediction goes here -->
      </td>
    </tr>
  </tbody>
</table>

<script src="/scripts/projected-savings.js"></script>

<%- include("partials/footer"); -%>
